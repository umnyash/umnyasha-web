extends ../../layout/layout

block variables
  - var title = 'Свойство imageSmoothingQuality.'
  - var pageClass = 'article-page'

block content
  main
    article.article.container
      h1= title
      p Свойство #[b imageSmoothingQuality] — качество сглаживания изображения.
      p Значения:
      ul
        li #[b 'low'] — (по умолчанию) низкое.
        li #[b 'medium'] — среднее.
        li #[b 'high'] — высокое.
